#include<stdio.h>
#include<ctype.h>
char vowel(ch);

int main()
{
    char ch;
    printf("\n Enter any character :");
    scanf("%c", &ch);
    vowel(ch);
    return 0;
}

char vowel(ch)

{
    
    switch(ch)
  {
    case 'A':
    case 'a':
    case 'E':
    case 'e':
    case 'I':
    case 'i':
    case 'O':
    case 'o':
    case 'U':
    case 'u':
    printf(" \n %c is a VOWEL",ch);
    break;
    
    default:
    if((ch>'A'&& ch<'Z') || (ch>'a'&& ch<'z'))
    printf(" \n %c is a consonant",ch);
    else if(isdigit(ch)>0)
    printf("%c is a digit",ch);
    else 
    printf("%c is a special character", ch);
    }

}
