// Program to print the sum of squares of first n odd numbers.
#include<stdio.h>
int sq_odd(int i, int n ,int s);
int main()
{
    int i=1 ,n,s=0;
    printf(" Enter the value of n : ");
    scanf("%d",&n);
    sq_odd(i,n,s);
    return 0;
}

int sq_odd(int i, int n, int s)

{
    while(i<=n)
    {
    if(i%2==0)
    printf("\n\t%d",i);
    if(i%2!=0)
    s += (i*i);
    i++;
    }
    printf("\n\n Sum of squares of first %d odd numbers : %d\n",n,s);
    return s;
}
