#include<stdio.h>

int main()
{
    char str1[100] , str2[100];
    int i=0 , j=0;
    printf("Enter the first string :");
    gets(str1);
    printf("Enter the second string :");
    gets(str2);
    
    while(str2[i] != '\0')
    i++;
    while(str1[j] != '\0')
    {
      str2[i] = str1[j];
      i++;
      j++;
    }
    
    str2[i] ='\0';
    printf("The final string after copying one string to another string is :\n");
    puts(str2);
    printf("The length of the final string is : %d",i);
    return 0;
}
