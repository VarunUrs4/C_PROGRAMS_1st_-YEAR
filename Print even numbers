#include<stdio.h>
int even(int i ,int m ,int n);
int main()
{
    int i ,m ,n ;
    printf("Enter the value of m :");
    scanf("%d",&m);
    printf("\nEnter the value of n :");
    scanf("%d",&n);
    printf("\n Even numbers :");
    even( i , m , n);
    return 0;
}

int even(int i ,int m ,int n)

{
    for(i=m;i<=n;i++)
    {
    if(i%2==0)
    printf(" %d ",i);
    }
    return ;
}
